<ion-header>
  <ion-toolbar>
    <div class="container">
      <div class="head">
        <div class="head__expand">
          <button class="head__button" type="button" (click)="navigateBack()">
            <svg width="24" height="24" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
              <path fill-rule="evenodd" clip-rule="evenodd" d="M16.1862 19.7188C16.6046 19.3439 16.6046 18.7361 16.1862 18.3612L9.08666 12L16.1862 5.63882C16.6046 5.26392 16.6046 4.65608 16.1862 4.28118C15.7678 3.90627 15.0894 3.90627 14.671 4.28118L6.81381 11.3212C6.3954 11.6961 6.3954 12.3039 6.81381 12.6788L14.671 19.7188C15.0894 20.0937 15.7678 20.0937 16.1862 19.7188Z"/>
            </svg>
          </button>
        </div>
        <div class="head__title">{t}Корзина{/t}</div>
        <div class="head__expand_right">
          <button
                  type="button"
                  class="cart-clear"
                  (click)="clearCart()"
                  *ngIf="!inLoading && cart && cart.items && cart.items.length"
          >{t}Очистить{/t}</button>
        </div>
      </div>
    </div>
  </ion-toolbar>
</ion-header>

<ion-content [fullscreen]="true">
  <div id="cart" class="">
    <ion-refresher slot="fixed" (ionRefresh)="doRefresh($event)">
      <ion-refresher-content refreshingSpinner="circles">
      </ion-refresher-content>
    </ion-refresher>
    <div class="section">
      <div class="container">
        <div *ngIf="inLoading">
          <app-cart-skeleton></app-cart-skeleton>
        </div>
        <div *ngIf="!inLoading && cart && cart.items && cart.items.length">
          <div *ngIf="cart.getMinOrderAmount()">
            <div class="cart-progress__text">{t}Мин. сумма заказа{/t} <span class="cart-progress__sum" [innerHTML]="cart.getMinOrderAmount()"></span></div>
            <div class="cart-progress__wrapper" [ngClass]="cart.checkMinOrderAmountProgress() ? 'complete' : ''">
              <div class="cart-progress__bar">
                <div class="cart-progress__amount" *ngIf="cart.getMinOrderAmountLeft()">
                  {t}Осталось{/t} <span class="cart-progress__sum" [innerHTML]="cart.getMinOrderAmountLeft()"></span>
                </div>
                <div class="cart-progress__fill" [style.width.%]="cart.getMinOrderAmountProgress()"></div>
              </div>
              <div class="cart-progress__check"></div>
            </div>
          </div>
          <ion-grid>
            <ion-row>
              <ion-col size-md="6" size="12">
                <div *ngIf="cart.items && cart.items.length">
                  <div
                          class="cart-item"
                          [ngClass]="cartItem.getError() ? 'cart-item_error' : ''"
                          *ngFor="let cartItem of cart.items"
                  >
                    <div class="cart-item__info">
                      <div class="cart-item__info" (click)="navigateTo('/tabs/catalog/product/' + cartItem.entityId)">
                        <div class="cart-item__img" *ngIf="cartItem.getImage()">
                          <canvas width="130" height="96"></canvas>
                          <ion-img [src]="cartItem.getImage()['small_url']" alt=""></ion-img>
                        </div>
                        <div class="cart-item__title_offer">
                          <div class="cart-item__title" [innerHTML]="cartItem.title"></div>
                          <div class="cart-item__offer fz-12 margin-8-top" *ngIf="cartItem.offerValues">
                            <div *ngFor="let item of cartItem.offerValues" class="c-primary-shady">
                              <span>{ { item.title } }: { { item.value } }</span>
                            </div>
                          </div>
                        </div>
                      </div>
                      <button type="button" class="cart-item__del" (click)="removeItem(cartItem.id)">
                        <svg width="20" height="20" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
                          <path fill-rule="evenodd" clip-rule="evenodd" d="M19.7758 4.22932C20.0767 4.53306 20.0744 5.02325 19.7707 5.32418L13.0323 12L19.7707 18.6758C20.0744 18.9767 20.0767 19.4669 19.7758 19.7707C19.4749 20.0744 18.9847 20.0767 18.6809 19.7758L11.9323 13.0898L5.31907 19.6416C5.01532 19.9425 4.52514 19.9402 4.22421 19.6365C3.92328 19.3327 3.92557 18.8425 4.22932 18.5416L10.8322 12L4.22934 5.4584C3.92559 5.15747 3.92331 4.66728 4.22424 4.36353C4.52516 4.05978 5.01535 4.0575 5.3191 4.35843L11.9323 10.9102L18.6809 4.22421C18.9847 3.92328 19.4748 3.92557 19.7758 4.22932Z" stroke="#1B1918" stroke-width="0.3" stroke-linecap="round"/>
                        </svg>
                      </button>
                    </div>
                    <div class="cart-item__price-wrap">
                      <div>
                        <div class="cart-item__price-old" *ngIf="cartItem.baseCost && cartItem.baseCost !== cartItem.cost" [innerHTML]="cartItem.baseCost"></div>
                        <div class="cart-item__price c-dark-red" [innerHTML]="cartItem.cost"></div>
                      </div>
                      <div>
                        <div class="cart-amount">
                          <button type="button" (click)="decreaseQuantityCartItem(cartItem)">
                            <svg width="12" height="12" viewBox="0 0 12 12"
                                 xmlns="http://www.w3.org/2000/svg">
                              <path d="M10.4752 6.46875H1.47516V4.96875H10.4752V6.46875Z"/>
                            </svg>
                          </button>
                          {* <div class="cart-amount__input">
                             <input
                                     type="number"
                                     [step]="cartItem.amountStep"
                                     [(ngModel)]="cartItem.amount"
                                     (ngModelChange)="changeAmount(cartItem)"
                             >
                             <span *ngIf="cartItem.unit" [innerHTML]="cartItem.unit"></span>
                           </div>*}
                          <div class="cart-amount__input" (click)="openSetAmount(cartItem)">
                            <span [innerHTML]="cartItem.amount"></span>
                            <span *ngIf="cartItem.unit" [innerHTML]="cartItem.unit"></span>
                          </div>
                          <button type="button" (click)="increaseQuantityCartItem(cartItem)">
                            <svg width="12" height="12" viewBox="0 0 12 12"
                                 xmlns="http://www.w3.org/2000/svg">
                              <path d="M10.7326 6.94364H6.87549V10.8008H5.58978V6.94364H1.73264V5.65792H5.58978V1.80078H6.87549V5.65792H10.7326V6.94364Z"/>
                            </svg>
                          </button>
                        </div>
                      </div>
                    </div>
                    <div class="cart-item__error" *ngIf="cartItem.getError()">
                      <svg width="20" height="20" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
                        <path fill-rule="evenodd" clip-rule="evenodd" d="M12 19C15.866 19 19 15.866 19 12C19 8.13401 15.866 5 12 5C8.13401 5 5 8.13401 5 12C5 15.866 8.13401 19 12 19ZM12 20C16.4183 20 20 16.4183 20 12C20 7.58172 16.4183 4 12 4C7.58172 4 4 7.58172 4 12C4 16.4183 7.58172 20 12 20Z"/>
                        <path d="M12 8.0274C11.6548 8.0274 11.375 8.30722 11.375 8.6524V12.6772C11.375 13.0224 11.6548 13.3022 12 13.3022C12.3452 13.3022 12.625 13.0224 12.625 12.6772V8.6524C12.625 8.30722 12.3452 8.0274 12 8.0274Z"/>
                        <path d="M12 15.7549C12.466 15.7549 12.8438 15.3772 12.8438 14.9112C12.8438 14.4452 12.466 14.0674 12 14.0674C11.534 14.0674 11.1562 14.4452 11.1562 14.9112C11.1562 15.3772 11.534 15.7549 12 15.7549Z"/>
                      </svg>
                      <span class="margin-8-left" [innerHTML]="cartItem.getError()"></span>
                    </div>
                  </div>
                </div>
              </ion-col>
              <ion-col size-md="6" size="12">
                <div class="cart-total margin-24-left-table">
                  <div class="margin-16-bottom" *ngIf="cart.hasError && (cart.errors && cart.errors.length)">
                    <div class="form__error" *ngFor="let error of cart.errors">
                      <svg width="20" height="20" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
                        <path fill-rule="evenodd" clip-rule="evenodd" d="M12 19C15.866 19 19 15.866 19 12C19 8.13401 15.866 5 12 5C8.13401 5 5 8.13401 5 12C5 15.866 8.13401 19 12 19ZM12 20C16.4183 20 20 16.4183 20 12C20 7.58172 16.4183 4 12 4C7.58172 4 4 7.58172 4 12C4 16.4183 7.58172 20 12 20Z"/>
                        <path d="M12 8.0274C11.6548 8.0274 11.375 8.30722 11.375 8.6524V12.6772C11.375 13.0224 11.6548 13.3022 12 13.3022C12.3452 13.3022 12.625 13.0224 12.625 12.6772V8.6524C12.625 8.30722 12.3452 8.0274 12 8.0274Z"/>
                        <path d="M12 15.7549C12.466 15.7549 12.8438 15.3772 12.8438 14.9112C12.8438 14.4452 12.466 14.0674 12 14.0674C11.534 14.0674 11.1562 14.4452 11.1562 14.9112C11.1562 15.3772 11.534 15.7549 12 15.7549Z"/>
                      </svg>
                      <span class="margin-8-left" [innerHTML]="error"></span>
                    </div>
                  </div>
                  <div *ngIf="showCouponField">
                    <div class="cart-total__promocode" *ngIf="cart.coupons && !cart.coupons.length">
                      <div class="fw-bold margin-16-bottom">{t}Скидочный купон{/t}</div>
                      <div class="cart-total__promocode-input">
                        <input type="text" [(ngModel)]="cart.couponValue" [value]="cart.couponValue">
                        <button type="button" (click)="addCoupon()">{t}Применить{/t}</button>
                      </div>
                    </div>
                    <div class="cart-total__promocode" *ngIf="cart.coupons && cart.coupons.length > 0">
                      <div class="fw-bold margin-16-bottom">{t}Скидочный купон{/t}</div>
                      <div class="cart-total__promocode-coupon" *ngFor="let coupon of cart.coupons">
                        <span class="cart-total__promocode-couponvalue">{t}Скидочный купон{/t} <b [innerHTML]="coupon.code"></b></span>
                        <button type="button" class="cart-total__promocode-deletecoupon" (click)="removeItem(coupon.id)">{t}Удалить{/t}</button>
                      </div>
                    </div>
                  </div>
                  {if $client_version >= 2.8}
                    <div class="cart-total__bonuses" *ngIf="useBonuses && isAuth">
                      <h3>{t}Бонусы{/t}</h3>

                      <div class="cart-total__use-bonuses-item" *ngIf="cart.userBonuses && cart.userBonuses > 0">
