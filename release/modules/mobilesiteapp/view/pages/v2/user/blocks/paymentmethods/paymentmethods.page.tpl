<ion-header>
  <ion-toolbar>
    <div class="container">
      <div class="head">
        <div class="head__expand">
          <button class="head__button" type="button" (click)="navigateBack()">
            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
              <path fill-rule="evenodd" clip-rule="evenodd" d="M16.1862 19.7188C16.6046 19.3439 16.6046 18.7361 16.1862 18.3612L9.08666 12L16.1862 5.63882C16.6046 5.26392 16.6046 4.65608 16.1862 4.28118C15.7678 3.90627 15.0894 3.90627 14.671 4.28118L6.81381 11.3212C6.3954 11.6961 6.3954 12.3039 6.81381 12.6788L14.671 19.7188C15.0894 20.0937 15.7678 20.0937 16.1862 19.7188Z" fill="#1B1B1F"/>
            </svg>
          </button>
        </div>
        <div class="head__title">{t}Мои привязанные карты{/t}</div>
        <div class="head__expand_right"></div>
      </div>
    </div>
  </ion-toolbar>
</ion-header>

<ion-content [fullscreen]="true">
  <div id="paymentmethods">
    <div class="section">
      <div class="container">
        <ion-grid>
          <ion-row>
            <ion-col size-lg="5" size-md="6" class="ion-hide-md-down">
              <div class="menu">
                <app-lk-menu></app-lk-menu>
              </div>
            </ion-col>
            <ion-col size-lg="5" size-md="6" offset-lg="1" size="12">
              <div *ngIf="inLoading">
                <app-paymentmethods-skeleton></app-paymentmethods-skeleton>
              </div>

              <div class="margin-16-left-table" *ngIf="!inLoading">
                <div *ngIf="payments && payments.length">
                  <div *ngFor="let payment of payments">
                    <div class="ion-text-center fw-medium c-dark margin-8-top" *ngIf="payment.savedMethodList && payment.savedMethodList.length" [innerHTML]="payment.title"></div>
                    <div *ngIf="payment.savedMethodList && payment.savedMethodList.length">
                      <div class="card-item" *ngFor="let card of payment.savedMethodList">
                        <div>
                          <div class="card-item__icon">
                            <span [innerHTML]="card.subtype"></span>
                          </div>
                          <div class="card-item__number">**** <span [innerHTML]="card.data.last4"></span></div>
                        </div>
                        <div>
                          <div class="card-item__main" *ngIf="card.is_default == 1">основная</div>
                          <div class="card-item__main" *ngIf="card.is_default == 0"><a (click)="setDefaultPaymentMethod(card)">{t}сделать основной{/t}</a></div>
                          <button class="card-item__del" type="button" (click)="deletePaymentMethod(card, payment)">
                            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                              <path fill-rule="evenodd" clip-rule="evenodd" d="M19.7758 4.22932C20.0767 4.53306 20.0744 5.02325 19.7707 5.32418L13.0323 12L19.7707 18.6758C20.0744 18.9767 20.0767 19.4669 19.7758 19.7707C19.4749 20.0744 18.9847 20.0767 18.6809 19.7758L11.9323 13.0898L5.31907 19.6416C5.01532 19.9425 4.52514 19.9402 4.22421 19.6365C3.92328 19.3327 3.92557 18.8425 4.22932 18.5416L10.8322 12L4.22934 5.4584C3.92559 5.15747 3.92331 4.66728 4.22424 4.36353C4.52516 4.05978 5.01535 4.0575 5.3191 4.35843L11.9323 10.9102L18.6809 4.22421C18.9847 3.92328 19.4748 3.92557 19.7758 4.22932Z" fill="#1B1B1F" stroke="#1B1918" stroke-width="0.3" stroke-linecap="round"/>
                            </svg>
                          </button>
                        </div>
                      </div>

                      <button type="submit" class="button button_primary w-100 margin-40-top margin-40-bottom" (click)="addNewPaymentMethod(payment)">{t}Привязать новую карту{/t}</button>
                    </div>

